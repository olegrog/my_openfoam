FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      q1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 0 -3 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    bottom
    {
        type zeroGradient;
    }
    top
    {
        type zeroGradient;
    }
    leftAndRight
    {
        type empty;
    }
    frontAndBack
    {
        type empty;
    }
}

