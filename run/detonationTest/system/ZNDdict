FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      ZNDdict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

normal      (1 0 0);
origin      (10 0 0);

solver      RKDP45;
relTol      1e-5;

