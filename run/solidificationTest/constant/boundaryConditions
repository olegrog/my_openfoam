FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      boundaryConditions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField       uniform 0;

boundaryField {
    bottom {
        type        zeroGradient;
    }
    top {
        type        zeroGradient;
    }
    left
    {
        type cyclic;
        neighbourPatch right;
    }
    right
    {
        type cyclic;
        neighbourPatch left;
    }
    frontAndBack {
        type        empty;
    }
}

// ************************************************************************* //
